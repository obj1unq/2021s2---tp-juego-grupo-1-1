import wollok.game.*
import personajes.*
import complementos.*
import inicio.*


describe "Pokemon test" {

  method initialize(){
    game.clear()
    game.addVisual(charmander)
  }

  test "Baya sube energia, trampa baja" {
    const trampaEnCamino = new Trampa(position = game.at(7,6), energiaQueBrinda = -15, defensaQueBrinda = -15)
    charmander.meEncontro(trampaEnCamino)
    assert.equals(85,charmander.energia())
    assert.equals(85,charmander.defensa())
  }
  
}